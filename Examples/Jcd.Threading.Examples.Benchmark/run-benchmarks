#!/bin/bash
mkdir -p ./benchmarks
rm -R ./benchmarks/
dotnet run -c Release -f net8.0 -- -a ./benchmarks/out --warmupCount 1 --minIterationCount 5 --maxIterationCount 10 --runtimes net8.0 net462 --filter "*"
rm ./benchmarks/out/results/*.csv
rm ./benchmarks/out/results/*.html
rm ./benchmarks/out/*.log